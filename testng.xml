<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite">
<!-- Need to change the target URL and Respective browser for execution -->
<parameter name="appURL" value="http://localhost:8080/"/>
<parameter name="browserType" value="chrome"/>
<listeners>
       <listener class-name="testbase.TestListener"/>
  </listeners>
  <test name="Test">
    <classes>
      <class name="functional.VerifyHomeImageTest"/>
      <class name="functional.VerifyExistingVeterinariansTest"/>
      <class name="functional.verifyAddNewOwnerAndPetTest"/>
      <class name="functional.VerifyExistingOwnersTest"/>
    </classes>
  </test> 
</suite>
